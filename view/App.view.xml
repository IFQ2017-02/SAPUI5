<?xml version="1.0" encoding="utf-8"?>
<mvc:View
   xmlns="sap.m"
   xmlns:mvc="sap.ui.core.mvc"
   xmlns:core="sap.ui.core"
   controllerName="hts.ifq2017-02.walkthrough.controller.App">
   <Text text="Hello World aus der XML-View"/>
   <core:HTML content="&lt;br&gt;&lt;br&gt;"/>
   <Link text="Auf zu Google!" href="https://google.com/"/>
   <VBox>
      <Text text="Hello World nochmal in VBox"/>
      <Link text="Auf zu Google!" href="https://google.com/"/>
   </VBox>
   <core:HTML content="&lt;br&gt;&lt;br&gt;"/>
   <VBox>
      <Button text="Standard-Meldung" press=".onAlert"/>
      <Button text="MessageToast" press=".onMessageToast"/>
      <Button text="MessageBox" press=".onMessageBox"/>
      <Button text="Fehler-Meldung" press=".onError"/>
   </VBox>
</mvc:View>